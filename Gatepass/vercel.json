{
  "version": 2,
  "builds": [
    {
      "src": "hostel_gatepass/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "hostel_gatepass/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hostel_gatepass.settings"
  }
}
